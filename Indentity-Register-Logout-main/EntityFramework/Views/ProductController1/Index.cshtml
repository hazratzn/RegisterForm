@model List<Product>



<section id="products">
    <div class="container">
        <input type="hidden" name="name" id="product-count" value="@ViewBag.ProductCount" />
        <div class="row" id="parent">
            <partial name="_ProductsPartialView" />
        </div>
        <div class="d-flex justify-content-center">

            <button class="btn btn-primary" id="show-more">Show More</button>
        </div>
    </div>
</section>

@await Component.InvokeAsync("Blog");
